<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper
        PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
        "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.itheima.mm.dao.QuestionItemDao">

    <insert id="add" parameterType="QuestionItem">
        insert into t_question_item(id,content,imgUrl,isRight,questionId)
        values(null,#{content},#{imgUrl},#{isRight},#{questionId})
    </insert>
</mapper>